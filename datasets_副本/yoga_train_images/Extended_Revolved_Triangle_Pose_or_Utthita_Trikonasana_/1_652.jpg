    event.preventDefault();
    $(".error-text").hide();
    var emailReg = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    var phoneRegEx = /(?:\d{1}\s)?\(?(\d{3})\)?-?\s?(\d{3})-?\s?(\d{4})/;
    if (!emailReg.test(email)) {
      $("#email-error").show();
    } else if (phone !== '' && !phoneRegEx.test(phone)) {
      $("#phone-error").show();
    } else {
      $('#emailCaptureForm').hide()
      $('.success').show();
      if (name == '' || name == "undefined") {
        name = "not selected";
      }
      if (external_url !== null && external_url !== '') {
        window.open(external_url, "_blank");
      }
      if (phone == '' || phone == "undefined") {
        phone = "not selected";
      }
      if (pro_code == '' || pro_code == "undefined") {
        pro_code = "not selected";
      }
      if (option_dropdown == '' || option_dropdown == "undefined") {
        option_dropdown = "not selected";
      }
      let lrEmail = encodeURIComponent(email);
      var response;
      var res_user_id;
      var res;
      var full_name;

      await fetch('https://lr.toneitup.com/', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify({ email })
      })
      .then(response => response.json())
      .then(res => {
        console.log('Success:', res);
        response = res.ErrorCode;
        res_user_id = res.Uid;
        full_name = res.FullName;
      })
      .catch((error) => {
        console.error('Error:', error);
      });

      if (res_user_id !== null && typeof res_user_id !== 'undefined' && response !== '1023') { //if we know them, we use the info from loginRadius
        braze.changeUser(res_user_id);
      }

      braze.getUser().setEmail(email);
      braze.logCustomEvent('Contact Form Submitted', {
        formId: form_id,
        email: email,
        campaignId: campaign_id,
        name: name
      });
      // comment
    }
  }
