	require(["jquery"], function($) {
		var root = $('[data-sv-mainnav]');
		var siteroot = $('body');
		var secondaryNav = $(siteroot).find('[data-sv-industrynav]').html();

		$('.main-3col-section div:last-child, .more-items div:last-child').addClass("end");

		if(typeof secondaryNav === 'undefined') {
			$(root).find('.nav-list').addClass('no-secondary');
		}

		$('.subnav-toggle').on('click',function(){
			$(this).toggleClass('active');
			$(this).parent().find('.mobile-children').slideToggle();
			$(this).parent().toggleClass('open');
			$(this).parent().toggleClass('selected');
		});

	// Mobile Nav

	$('.hamburger').on("click", function() {
		$(this).toggleClass("active");
		$(this).parent().toggleClass("active");
		$('[data-sv-mainnav]').toggleClass("active");
	});


	$('[data-menu-toggle-parent]').on('click',function(){
		$(this).toggleClass('active');
		$(this).next('.mobile-children').slideToggle();
		$(this).parent().find('.parent-arrow').toggleClass('open');
		$(this).parent().toggleClass('selected');
