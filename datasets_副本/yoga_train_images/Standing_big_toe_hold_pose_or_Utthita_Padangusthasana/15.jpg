        window.addEventListener('resize', updateMenu);
        updateMenu(); // Initial setup

        // After adjustments, make the navigation visible
        setTimeout(() => {
          navs.forEach(nav => nav.classList.add('is-visible'));
          updateMenu(); // Adjust menu items as needed
        }, 100);
      });
